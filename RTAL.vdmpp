class RTAL
instance variables
	public entry : seq of char;
	public static iteration: nat := 0;
operations
--Constructor-Create a new string entry Log
	public RTAL: seq of char ==> RTAL
	RTAL(message) == (entry := message; 
										iteration:=iteration+1;
										return self)
	pre message <> []
	post entry = message
end RTAL